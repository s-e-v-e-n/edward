<?xml version="1.0" encoding="utf-8"?>
<root xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <!-- Kommandos die mit "Axis" oder "Thrust" enden sind meist an analoge Eingänge wie Joysticks gebunden, daher sind sie hier lediglich der Vollständigkeit halber und für spätere Funktionen aufgeführt. Für die meisten Achsen gibt es jedoch auch Kommandos für einen digitale Eingang wie eine Taste auf der Tastatur oder ein Button auf dem Joystick.

    Fokussieren sie sich beim anpassen auf die "Buttons".

    -->
    <KeyboardLayout>
        de-DE
    </KeyboardLayout>
    <!-- Maus zurücksetzen, manchmal hindert einen die Tischkante am richtigen Lenkmanöver. Dafür ist dieses Kommando -->
    <MouseReset>
        Maus zurücksetzen
    </MouseReset>

    <!-- Gierachse. Wie im Auto lenkt es die Nase des Fahrzeugs nach links oder rechts-->
    <YawAxis>
        Gierachse
    </YawAxis>
    <!-- nach links ziehen -->
    <YawLeftButton>
        nach links ziehen
    </YawLeftButton>
    <!-- nach rechts ziehen -->
    <YawRightButton>
        nach rechts ziehen
    </YawRightButton>

    <!-- Gier zu rollen, bedeutet: wenn sie die Schiffsnase nach links lenken neigt es sich gleichzeitig etwas in die selbe Richtung. Motorradfahrer sind so etwas gewohnt. -->
    <YawToRollButton>
        Gier zu rollen
    </YawToRollButton>

    <!-- Rollachse -->
    <RollAxis>
        Rollachse
    </RollAxis>
    <!-- links rollen -->
    <RollLeftButton>
        links rollen
    </RollLeftButton>
    <!-- rechts rollen -->
    <RollRightButton>
        rechts rollen
    </RollRightButton>

    <!-- Nickachse. Diese Achse kennen sie nur in den seltensten Fällen aus dem Straßenverkehr, es sei denn sie sind Motorradrennfahrer auf der Zielgeraden oder Dragsterpilot. Die Schiffsnase geht dabei entweder hoch oder runter.-->
    <PitchAxis>
        Nickachse
    </PitchAxis>
    <!-- hoch ziehen -->
    <PitchUpButton>
        hoch ziehen
    </PitchUpButton>
    <!-- runter drücken -->
    <PitchDownButton>
        runter drücken
    </PitchDownButton>

    <!-- Horizontalschub. Für die Spieler von Egoshootern: Das hier ist das Äquivalent zu "Strafe". Seitwärtsbewegung ohne Drehung des Schiffs. -->
    <LateralThrust>
        Horizontalschub
    </LateralThrust>
    <!-- Schub links -->
    <LeftThrustButton>
        Schub links
    </LeftThrustButton>
    <!-- Schub rechts -->
    <RightThrustButton>
        Schub rechts
    </RightThrustButton>

    <!-- Vertikalschub. Wie im Aufzug. -->
    <VerticalThrust>
        Vertikalschub
    </VerticalThrust>
    <!-- Schub hoch -->
    <UpThrustButton>
        Schub hoch
    </UpThrustButton>
    <!-- Schub runter -->
    <DownThrustButton>
        Schub runter
    </DownThrustButton>

    <!-- Frontalschub. Die meistgenutze Bewegungsrichtung in Fahrzeugen. Vorwärts bzw. Rückwärts -->
    <AheadThrust>
        Frontalschub
    </AheadThrust>
    <!-- Schub nach vorn -->
    <ForwardThrustButton>
        Schub nach vorn
    </ForwardThrustButton>
    <!-- Schub zurück -->
    <BackwardThrustButton>
        Schub zurück
    </BackwardThrustButton>

    <!-- Schubachse, das Gaspedal -->
    <ThrottleAxis>
        Schubachse
    </ThrottleAxis>
    <!-- Schub umkehren oder den Rückwärtsgang einlegen -->
    <ToggleReverseThrottleInput>
        Schub umkehren
    </ToggleReverseThrottleInput>
    <!-- vorwärts -->
    <ForwardKey>
        vorwärts
    </ForwardKey>
    <!-- rückwärts -->
    <BackwardKey>
        rückwärts
    </BackwardKey>

    <!-- Mit den nachfolgenden 9 Kommandos steuern sie das "Gaspedal" oder genauer den Schub und den Umkehrschub in Stufen. -->
    <!-- Volle Kraft zurück -->
    <SetSpeedMinus100>
        Volle Kraft zurück
    </SetSpeedMinus100>
    <!-- Dreiviertel Kraft zurück -->
    <SetSpeedMinus75>
        Dreiviertel Kraft zurück
    </SetSpeedMinus75>
    <!-- Halbe Kraft zurück -->
    <SetSpeedMinus50>
        Halbe Kraft zurück
    </SetSpeedMinus50>
    <!-- Viertel Kraft zurück -->
    <SetSpeedMinus25>
        Viertel Kraft zurück
    </SetSpeedMinus25>
    <!-- Alle Maschinen stop -->
    <SetSpeedZero>
        Alle Maschinen stop
    </SetSpeedZero>
    <!-- Viertel Kraft voraus -->
    <SetSpeed25>
        Viertel Kraft voraus
    </SetSpeed25>
    <!-- Halbe Kraft voraus -->
    <SetSpeed50>
        Halbe Kraft voraus
    </SetSpeed50>
    <!-- Dreiviertel Kraft voraus -->
    <SetSpeed75>
        Dreiviertel Kraft voraus
    </SetSpeed75>
    <!-- Volle Kraft voraus -->
    <SetSpeed100>
        Volle Kraft voraus
    </SetSpeed100>

    <!-- Flugassistent ein und ausschalten-->
    <ToggleFlightAssist>
        Flugassistent
    </ToggleFlightAssist>

    <!-- BoostJuice, der Saft sei mit dir -->
    <UseBoostJuice>
        Nachbrenner
    </UseBoostJuice>
    <!-- Impuls Warp Kombination. Name ist Programm. Hier ist die Besonderheit zu sehen wenn man englische Begriffe in deutscher Spracherkennung nutzt, muss man etwas erfinderisch werden was die Schreibweise angeht. Dann versteht man sich besser mit seinem Computer. -->
    <HyperSuperCombination>
        Impuls Woarp Kombination
    </HyperSuperCombination>
    <!-- Impulsantrieb -->
    <Supercruise>
        Impulsantrieb
    </Supercruise>
    <!-- Woarpantrieb;Hyperraumantrieb -->
    <Hyperspace>
        Woarpantrieb;Hyperraumantrieb
    </Hyperspace>
    <!-- Rotationskorrektur -->
    <DisableRotationCorrectToggle>
        Rotationskorrektur
    </DisableRotationCorrectToggle>

    <!-- Ziel anvisieren -->
    <SelectTarget>
        Ziel anvisieren
    </SelectTarget>
    <!-- nächstes Ziel -->
    <CycleNextTarget>
